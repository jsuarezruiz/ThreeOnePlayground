<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ThreeOnePlayground.Views.EvaluateJavaScriptView"
             Title="Evaluate Javascript">
    <ContentPage.Content>
        <StackLayout>
            <WebView
                x:Name="EvaluateJsWebView"
                HeightRequest="300"
                Source="http://www.xamarin.com"/>
            <Button 
                x:Name="EvaluateJsBtn"
                Text="Evaluate Javascript"/>
            <Label
                x:Name="EvaluateJsLabel"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>